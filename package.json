{
  "name": "portfolio",
  "version": "3.0.0",
  "private": false,
  "author": "Lucas Bittencourt <lucasgdbittencourt@gmail.com>",
  "repository": "lucasgdb/portfolio",
  "license": "MIT",
  "jest": {
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/src/$1"
    }
  },
  "scripts": {
    "delete:node_modules": "find . -name \"node_modules\" -exec rm -rf '{}' +",
    "delete:build": "find . -name \"build\" -exec rm -rf '{}' +",
    "clear": "yarn delete:node_modules && yarn delete:build",
    "dev": "webpack serve --config webpack.dev.ts",
    "build": "webpack --config webpack.prod.ts",
    "format": "prettier --ignore-path .eslintignore --write --check ."
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-animations": "^1.0.0",
    "react-dom": "^18.2.0",
    "react-ga": "^3.3.1",
    "react-genie": "^0.2.2",
    "react-genie-styled-components": "^0.1.1",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scrollspy": "^3.4.3",
    "styled-components": "^5.3.5",
    "vertical-timeline-component-for-react": "^1.0.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^6.3.1",
    "@types/node": "^18.6.1",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-scrollspy": "^3.3.5",
    "@types/styled-components": "^5.1.25",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.7.1",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
